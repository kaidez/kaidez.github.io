    "@babel/preset-react": "^7.27.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "gh-pages": "^6.3.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "moment": "^2.30.1"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "blog",
    "static-site"
